@1 = ~L'anna – nowy towarzysz dla IWD:EE~
@3 = ~L'anna~
@4 = ~Zapytana o przeszłość, L'ANNA opowiada o swojej rodzinie: leśnej elfce, która była jej matką oraz ojcu – dzikim elfie. Spędziła większość swojego życia w kniejach, doskonaląc umiejętności posługiwania się mieczem. Rzadko kiedy ogarnia ją strach podczas walki nawet z najpotężniejszymi przeciwnikami, lecz wie, jak to jest być sparaliżowaną z przerażenia. L'ANNA była naocznym świadkiem straszliwego pożaru, który pochłonął las, w którym żyła wraz z domem i rodziną. Utraciła najdroższe sobie osoby, ale nawet tak makabryczne zdarzenie nie złamało ducha elfki. Ćwiczyła coraz bardziej intensywnie i oddała się bogowi mythali, potężnej i pradawnej magii, mogącej ochronić wielkie połacie terenów oraz ich mieszkańców. Być może to właśnie utrata domu pozwoliła L'ANNIE odnaleźć prawdziwe powołanie.~
@5 = ~Nie... wszystko... nie tak.~ [L#LANN01]
@6 = ~Wskażę nam właściwą drogę.~ [L#LANN02]
@7 = ~Bywają chwile, w których nawet najwprawniejsi poszukiwacze przygód muszą zatrzymać się i odpocząć.~ [L#LANN03]
@8 = ~Moglibyśmy zrobić o wiele więcej niż tylko tkwić tu w oczekwianiu.~ [L#LANN04]
@9 = ~Za wszystko, co warto chronić!~ [L#LANN05]
@10 = ~Saltharan silth!~ [L#LANN06]
@11 = ~Mythrienie, miej mnie w opiece!~ [L#LANN07]
@12 = ~Koniec z tym!~ [L#LANN08]
@13 = ~Do walki!~ [L#LANN09]
@14 = ~Aaa!~ [L#LANN10]
@15 = ~Nie... Mythrienie... Proszę...~ [L#LANN11]
@16 = ~Nie... mogę... złapać... tchu.~ [L#LANN12]
@17 = ~Coż za urokliwe miejsce.~ [L#LANN13]
@18 = ~Mmm.~ [L#LANN14]
@19 = ~Ostrożnie!~ [L#LANN15]
@20 = ~Co też czeka nas dzisiaj?~ [L#LANN16]
@21 = ~Cicho. Musimy się upewnić, że nikt na nas nie czyha.~ [L#LANN17]
@22 = ~Jestem tutaj.~ [L#LANN18]
@23 = ~Słucham.~ [L#LANN19]
@24 = ~Każdy dobry dom zasługuje na ochronę.~ [L#LANN20]
@25 = ~Rozumiem, że mnie potrzebujesz.~ [L#LANN41]
@26 = ~Tak?~ [L#LANN21]
@27 = ~Jaki masz plan?~ [L#LANN22]
@28 = ~Mity żyją wiecznie.~ [L#LANN23]
@29 = ~To nie powinno być trudne.~ [L#LANN24]
@30 = ~Mythrienie, czuwaj nade mną.~ [L#LANN25]
@31 = ~Chyba mogę to zrobić.~ [L#LANN26]
@32 = ~Mhm.~ [L#LANN27]
@33 = ~Lepsze to niż bezczynność.~ [L#LANN28]
@34 = ~Skoro uważasz, że właśnie to powinnam uczynić...~ [L#LANN29]
@35 = ~Niech cię Dziewięć Piekieł pochłonie!~ [L#LANN30]
@36 = ~Ach!~ [L#LANN31]
@37 = ~Co...? Jak?!~ [L#LANN32]
@38 = ~Wybacz, ale nie jestem jucznym mułem.~ [L#LANN33]
@39 = ~Na wszystkie mity... Utraciłam zaklęcie!~ [L#LANN34]
@40 = ~Nie jestem przekonana do takich rozwiązań, ale... gotowe.~ [L#LANN35]
@41 = ~Nie zobaczą mnie.~ [L#LANN36]
@42 = ~Mam to. Możemy iść.~ [L#LANN37]
@43 = ~Pomścimy cię!~ [L#LANN38]
@44 = ~Nie pozwolę na zniszczenie kolejnego domu.~ [L#LANN39]
@45 = ~Mythale to o wiele więcej niż prosta magia. Dlatego nie mogę pozwolić im zniknąć.~
